$ cd ~

$ mkdir Assignment4

$ cd ./Assignment4/

$ mkdir FolderD FolderE

$ touch ./FolderD/file2.txt ./FolderE/file3.txt

$ ls ./FolderD ./FolderE
./FolderD:
file2.txt

./FolderE:
file3.txt

$ rm ./FolderD/file2.txt ./FolderE/file3.txt

$ ls ./FolderD ./FolderE
./FolderD:

./FolderE:

$ rmdir FolderD FolderE

$ ls
